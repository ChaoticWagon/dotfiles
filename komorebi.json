{
  "$schema": "https://raw.githubusercontent.com/LGUG2Z/komorebi/v0.1.24/schema.json",
  "app_specific_configuration_path": "$Env:USERPROFILE/applications.yaml",
  "window_hiding_behaviour": "Cloak",
  "cross_monitor_move_behaviour": "Insert",
  "default_workspace_padding": 2,
  "default_container_padding": 0,
  "border_padding": 0,
  "border_offset": -1,
  "active_window_border": true,
  "active_window_border_width": 3,
  "active_window_border_colours": {
    "single": "#cba6f7",
    "stack": "#cba6f7",
    "monocle": "#cba6f7"
  },
  "monitors": [
    {
      "workspaces": [
        { "name": "I", "layout": "UltrawideVerticalStack" }
      ]
    }
  ],
  "global_work_area_offset": { "bottom": 0, "left": 0, "right": 0, "top": 0 },
  "float_rules": [
    {
      "kind": "Exe",
      "id": "Zebar.exe",
      "matching_strategy": "Equals"
    }
  ]
}
